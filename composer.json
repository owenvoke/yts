{
    "name": "pxgamer/yts",
    "type": "library",
    "description": "A full wrapper for the YTS API.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "tightenco/collect": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\YTS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\YTS\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    }
}
